generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Endpoint {
  id          String      @id @default(uuid())
  title       String      @unique
  description String
  uri         String
  createdAt   DateTime    @default(now()) @map(name: "created_at")
  updatedAt   DateTime?   @map(name: "updated_at")

  @@map("endpoints")
}
